    agent any

    stages {
        stage('Build') {
            steps {
                sh 'echo "building....."' 
            }
        }
        stage('Testing') {
            steps {
                sh 'echo "Testing....."'
            }
        }
        stage('Deploy') {
            steps {
                sh 'echo "Deploying....."'
            }
        }
    }
}